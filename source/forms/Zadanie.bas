Option Compare Database
Option Explicit

Private Sub btnAnuluj_Click()
   Call saveRecord
   On Error Resume Next
   Call deleteRecord
   On Error GoTo 0
   DoCmd.Close acForm, Me.Name
End Sub

Private Sub btnDodaj_Click()
   Dim sql As String
   If jestOK = False Then Exit Sub
   Call saveRecord
   If policzOsoby > 0 Then MsgBox "Ta osoba jest już przypisana": Exit Sub
   sql = mkStr("Insert into osobyDoZadan(zadanie, osobyKontaktowe) values($1,$2)", Me.ID, Me.kombiOsoby)
   Call runSQL(sql)
   Call tworzTbOsoby
   Me.kombiOsoby = Null
End Sub

Function jestOK() As Boolean
   If Nz(Me.nazwaZadania, "") = "" Then
      MsgBox "Uzupełnij nazwę zadania"
   Else
      jestOK = True
   End If
End Function

Function policzOsoby() As Integer
   policzOsoby = DCount("*", "osobyDoZadan", mkStr("zadanie = $1 and osobyKontaktowe = $2", Me.ID, Me.kombiOsoby))
End Function

Sub tworzTbOsoby()
   Dim rec As Recordset, sql As String, tekst As String
   If IsNull(Me.ID) Then Exit Sub
   sql = "Select o.*, ok.*, odz.* " & _
         "from Organizacje As o inner join (osobyKontaktowe as ok inner join osobyDoZadan as odz ON ok.ID = odz.osobyKontaktowe) On o.ID = ok.firma " & _
         "where odz.zadanie = " & Me.ID
   Set rec = CurrentDb.OpenRecordset(sql, dbOpenDynaset)
   Do Until rec.EOF
      tekst = tekst & " | " & rec!imie & " " & rec!nazwisko & " (" & rec!nazwaFirmy & ")"
      rec.MoveNext
   Loop
   tekst = Right(tekst, Len(tekst) - 3)
   Me.tbOsoby = tekst
End Sub

Private Sub btnKasuj_Click()
   Dim sql As String
   If policzOsoby = 0 Then MsgBox "Ta osoba nie jest przypisana do zadania": Exit Sub
   sql = mkStr("Delete from osobyDoZadan Where zadanie = $1 and osobyKontaktowe = $2", Me.ID, Me.kombiOsoby)
   Call runSQL(sql)
   Call tworzTbOsoby
   Me.kombiOsoby = Null
End Sub

Private Sub btnZapisz_Click()
   DoCmd.Close acForm, Me.Name
End Sub

Private Sub Form_Load()
   Me.btnAnuluj.Visible = IsNull(Me.ID)
   Call tworzTbOsoby
End Sub
