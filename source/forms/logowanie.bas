Private Sub btnZaloguj_Click()
   Dim haslo As String
   haslo = MD5_string(Me.tbHaslo)
   If DCount("*", "Uzytkownicy", mkStr("login = '$1' and haslo = '$2'", Me.tbLogin, haslo)) = 1 Then
      Call setUser(Me.tbLogin)
   Else
      MsgBox "Brak dostępu"
      Me.tbHaslo = Null
      Me.tbHaslo.SetFocus
   End If
End Sub
